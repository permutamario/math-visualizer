{
  "id": "interactive",
  "name": "Interactive Shapes",
  "version": "1.0.0",
  "description": "Interactive shapes that can be clicked and dragged",
  "author": "Math Visualization Framework",
  "minFrameworkVersion": "1.0.0",
  "environment": {
    "type": "2d-event",
    "options": {}
  },
  "defaultSettings": {
    "defaultColor": "#3498db",
    "selectedColor": "#e74c3c",
    "snapToGrid": false,
    "gridSize": 20,
    "backgroundColor": "#f5f5f5"
  },
  "uiCategories": [
    {
      "id": "structural",
      "label": "Structural Parameters",
      "settings": ["snapToGrid", "gridSize"]
    },
    {
      "id": "visual",
      "label": "Visual Parameters",
      "settings": ["defaultColor", "selectedColor", "backgroundColor"]
    }
  ],
  "exportOptions": [
    {
      "id": "export-png",
      "label": "Export PNG",
      "type": "export"
    },
    {
      "id": "add-shape",
      "label": "Add Random Shape",
      "type": "action"
    },
    {
      "id": "reset-settings",
      "label": "Reset Settings",
      "type": "export"
    }
  ]
}
