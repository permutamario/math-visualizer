{
  "id": "circle",
  "name": "Circle Visualization",
  "version": "1.0.0",
  "description": "A sectioned circle visualization plugin for the Math Visualization Framework",
  "author": "Your Name",
  "minFrameworkVersion": "1.0.0",
  "defaultSettings": {
    "circleRadius": 100,
    "circleColor": "#4285f4",
    "circleOpacity": 1.0,
    "circleSections": 1,
    "backgroundColor": "#f5f5f5",
    "showBorder": true,
    "borderColor": "#000000",
    "borderWidth": 2
  },
  "uiCategories": [
    {
      "id": "structural",
      "label": "Structural Parameters",
      "settings": ["circleRadius", "circleSections"]
    },
    {
      "id": "visual",
      "label": "Visual Parameters",
      "settings": ["backgroundColor", "circleColor", "circleOpacity", "showBorder", "borderColor", "borderWidth"]
    }
  ],
  "exportOptions": [
    {
      "id": "export-png",
      "label": "Export PNG",
      "type": "export"
    },
    {
      "id": "reset-settings",
      "label": "Reset Settings",
      "type": "export"
    }
  ]
}
